@model IEnumerable<WebApi.SharedModels.Christiansø>
<div class="container p-3">
    <div class="col-4 text-">
        <a asp-action="Index" class="btn btn-primary w-20"><i class="bi bi-backspace"></i>  Tilbage til fuld liste</a>
    </div>
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width = 100%">
            <thead>
               <tr>
                    <th> ID</th>
                    <th> KildeBog </th>
                    <th>Side</th>
                    <th>AnkomstAfrejse</th>
                    <th>Kompani</th>
                    <th>Aar</th>
                    <th>Titel</th>
                    <th> Fornavn</th>
                    <th> Efternavn </th>
                    <th> Slægtsnavn </th>
                    <th>Fødested </th>
                    <th>FødtDato </th>
                    <th> FødtÅr </th>
                    <th>Alder</th>
                    <th> KomFra</th>
                    <th>Distrikt</th>
                    <th>Amt </th>
                    <th>By </th>
                    <th>LægdNr </th>
                    <th> PatentNr </th>
                    <th> LøbeNr </th>
                    <th> ExtraTjPåØen </th>
                    <th> TjenesteFra1852 </th>
                    <th>TjenesteTid</th>
                    <th>Premierløjtnant</th>
                    <th>SøLøjtnantSøOfficer</th>
                    <th>Tømmermand</th>
                    <th>Lodsmatros</th>
                    <th>LodsQvartersmester</th>
                    <th>Soldat</th>
                    <th>Konstabel</th>
                    <th>UnderKanoner</th>
                    <th>Arbejdsmand</th>
                    <th>Gift</th>
                    <th>Børn</th>
                    <th>Anmærkning</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                    <td width="5%">@item.ID</td>
                    <td width="5%">@item.KildeBog</td>
                    <td width="5%">@item.Side</td>
                    <td width="5%">@item.AnkomstAfrejse</td>
                    <td width="5%">@item.Kompani</td>
                    <td width="5%">@item.Aar</td>
                    <td width="5%">@item.Nr</td>
                    <td width="5%">@item.Titel</td>
                    <td width="5%">@item.Fornavn</td>
                    <td width="5%">@item.Efternavn</td>
                    <td width="5%">@item.Slægtsnavn</td>
                    <td width="5%">@item.Fødested</td>
                    <td width="5%">@item.FødtDato</td>
                    <td width="5%">@item.FødtÅr</td>
                    <td width="5%">@item.Alder</td>
                    <td width="5%">@item.KomFra</td>
                    <td width="5%">@item.Distrikt</td>
                    <td width="5%">@item.Amt</td>
                    <td width="5%">@item.By</td>
                    <td width="5%">@item.LægdNr</td>
                    <td width="5%">@item.PatentNr</td>
                    <td width="5%">@item.LøbeNr</td>
                    <td width="5%">@item.ExtraTjPåØen</td>
                    <td width="5%">@item.TjenesteFra1852</td>
                    <td width="5%">@item.Premierløjtnant</td>
                    <td width="5%">@item.SøLøjtnantSøOfficer</td>
                    <td width="5%">@item.Tømmermand</td>
                    <td width="5%">@item.Lodsmatros</td>
                    <td width="5%">@item.LodsQvartersmester</td>
                    <td width="5%">@item.Soldat</td>
                    <td width="5%">@item.Konstabel</td>
                    <td width="5%">@item.UnderKanoner</td>
                    <td width="5%">@item.Arbejdsmand</td>
                    <td width="5%">@item.Gift</td>
                    <td width="5%">@item.Konstabel</td>
                    <td width="5%">@item.Børn</td>
                    <td width="5%">@item.Anmærkning</td>


                        <td>
                            <div class=" btn-group" role="group">

                                <a asp-controller="Christian" asp-action="Edit" asp-route-id="@item.ID" class="btn btn-primary  mx-2"><i class="bi bi-pencil-square"></i>Redigere</a>
                            </div>
                        </td>
                        <td>
                            <div>
                                <a asp-controller="Christian" asp-action="Delete" asp-route-id="@item.ID" class="btn btn-danger  mx-1"><i class="bi bi-trash3"></i>Slet</a>
                            </div>
                        </td>

                    </tr>
                }

            </tbody>
        </table>
    }

    else
    {
        <p> Item Not Exist </p>
    }

</div>






