@model IEnumerable<WebApi.SharedModels.FT>
<div class="container p-3">
    <div class="col-4 text-">
        <a asp-action="Index" class="btn btn-primary w-20"><i class="bi bi-backspace"></i>  Tilbage til fuld liste</a>
    </div>
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width = 100%">
            <thead>
              <tr>
                    <th> ID</th>
                    <th> Fornavne</th>
                    <th> Efternavn </th>
                    <th>Fødested</th>
                    <th>Alder</th>
                    <th>GiftUgift </th>
                    <th>Stilling </th>
                    <th>Amt </th>
                    <th>SognAdresse </th>
                    <th>Løbenr </th>
                    <th>IndtastNr </th>
                    <th>Bemærkninger </th>
               </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                   <tr>
                        <td width="5%">@item.ID</td>
                        <td width="5%">@item.Fornavne</td>
                        <td width="5%">@item.Efternavn</td>
                        <td width="5%">@item.Fødested</td>
                        <td width="5%">@item.Alder</td>
                        <td width="5%">@item.GiftUgift</td>
                        <td width="5%">@item.Stilling</td>
                        <td width="5%">@item.Amt</td>
                        <td width="5%">@item.SognAdresse</td>
                        <td width="5%">@item.Løbenr</td>
                        <td width="5%">@item.IndtastNr</td>
                        <td width="5%">@item.Bemærkninger</td>

                        <td>
                            <div class=" btn-group" role="group">

                                <a asp-controller="Ft" asp-action="Edit" asp-route-id="@item.ID" class="btn btn-primary  mx-2"><i class="bi bi-pencil-square"></i>Redigere</a>
                            </div>
                        </td>
                        <td>
                            <div>
                                <a asp-controller="Ft" asp-action="Delete" asp-route-id="@item.ID" class="btn btn-danger  mx-1"><i class="bi bi-trash3"></i>Slet</a>
                            </div>
                        </td>

                    </tr>
                }

            </tbody>
        </table>
    }

    else
    {
        <p> Varen findes ikke </p>
    }

</div>






