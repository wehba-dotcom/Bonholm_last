@model WebApi.SharedModels.Begrav

<div class="card shadow border-0 mt-4">
    <div class=" card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">

                <h2 class="text-blabk-50 pl-3">TilFøjet Ny </h2>

            </div>

        </div>

    </div>
    <div class="card-body p-4">
        <form method="post" action="Create">
            <div class=" border p-3">
                <div class="form-group row text-center "></div>
                <div class="col">
                    <div class="row-12">
                        <div class="form-group col">
                            <div asp-validation-summary="All"></div>
                            <div class="mp-3">
                                <label asp-for="Protokol" class="form-label"></label>
                                <input asp-for="Protokol" class="form-control" />
                                <span asp-validation-for="Protokol" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="ProtokolNr" class="form-label"></label>
                                <input asp-for="ProtokolNr" class="form-control" />
                                <span asp-validation-for="ProtokolNr" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Side" class="form-label"></label>
                                <input asp-for="Side" class="form-control" />
                                <span asp-validation-for="Side" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Fornavn" class="form-label"></label>
                                <input asp-for="Fornavn" class="form-control" />
                                <span asp-validation-for="Fornavn" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Efternavn" class="form-label"></label>
                                <input asp-for="Efternavn" class="form-control" />
                                <span asp-validation-for="Efternavn" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Fødenavn" class="form-label"></label>
                                <input asp-for="Fødenavn" class="form-control" />
                                <span asp-validation-for="Fødenavn" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Stilling" class="form-label"></label>
                                <input asp-for="Stilling" class="form-control" />
                                <span asp-validation-for="Stilling" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Adresse" class="form-label"></label>
                                <input asp-for="Adresse" class="form-control" />
                                <span asp-validation-for="Adresse" class="text-danger"></span>
                            </div>
                           
                            <div class="mp-3">
                                <label asp-for="SognBy" class="form-label"></label>
                                <input asp-for="SognBy" class="form-control" />
                                <span asp-validation-for="SognBy" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Født" class="form-label"></label>
                                <input asp-for="Født" class="form-control" />
                                <span asp-validation-for="Født" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Død" class="form-label"></label>
                                <input asp-for="Død" class="form-control" />
                                <span asp-validation-for="Død" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Begravet" class="form-label"></label>
                                <input asp-for="Begravet" class="form-control" />
                                <span asp-validation-for="Begravet" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Pasning" class="form-label"></label>
                                <input asp-for="Pasning" class="form-control" />
                                <span asp-validation-for="Pasning" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Adresse2" class="form-label"></label>
                                <input asp-for="Adresse2" class="form-control" />
                                <span asp-validation-for="Adresse2" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Sogn" class="form-label"></label>
                                <input asp-for="Sogn" class="form-control" />
                                <span asp-validation-for="Sogn" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Sletning" class="form-label"></label>
                                <input asp-for="Sletning" class="form-control" />
                                <span asp-validation-for="Sletning" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Anmærkning" class="form-label"></label>
                                <input asp-for="Anmærkning" class="form-control" />
                                <span asp-validation-for="Anmærkning" class="text-danger"></span>
                            </div>
                           


                        </div>
                        <br />
                        <div class="row">
                            <div class="col-6 col-md-3">

                                <input type="submit" class="btn btn-primary w-75" value="Tilføj" />
                            </div>
                            <div class="col-6 col-md-3">
                                <a asp-action="Index" class="btn btn-primary w-75">Tilbage</a>
                            </div>

                        </div>

                </div>

            </div>

        </form>

    </div>

</div>
