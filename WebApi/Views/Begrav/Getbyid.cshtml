@model IEnumerable<WebApi.SharedModels.Begrav>
<div class="container p-3">
    <div class="col-4 text-">
        <a asp-action="Index" class="btn btn-primary w-20"><i class="bi bi-backspace"></i>  Tilbage til fuld liste</a>
    </div>
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width = 100%">
            <thead>
                <tr>
                    <th> ID</th>
                    <th> Protokol </th>
                    <th>ProtokolNr</th>
                    <th>Side</th>
                    <th> Fornavn</th>
                    <th> Efternavn </th>
                    <th>Fødenavn</th>
                    <th>Stilling </th>
                    <th>Adresse</th>
                    <th>SognBy</th>
                    <th>Født</th>
                    <th> Død </th>
                    <th>Begravet </th>
                    <th> Pasning </th>
                    <th> Adresse2</th>
                    <th>Sogn</th>
                    <th>Sletning </th>
                    <th>Anmærkning </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td width="5%">@item.ID</td>
                        <td width="5%">@item.Protokol</td>
                        <td width="5%">@item.ProtokolNr</td>
                        <td width="5%">@item.Side</td>
                        <td width="5%">@item.Fornavn</td>
                        <td width="5%">@item.Efternavn</td>
                        <td width="5%">@item.Fødenavn</td>
                        <td width="5%">@item.Stilling</td>
                        <td width="5%">@item.Adresse</td>
                        <td width="5%">@item.SognBy</td>
                        <td width="5%">@item.Født</td>
                        <td width="5%">@item.Død</td>
                        <td width="5%">@item.Begravet</td>
                        <td width="5%">@item.Pasning</td>
                        <td width="5%">@item.Adresse2</td>
                        <td width="5%">@item.Sogn</td>
                        <td width="5%">@item.Sletning</td>
                        <td width="5%">@item.Anmærkning</td>

                        <td>
                            <div class=" btn-group" role="group">

                                <a asp-controller="Allsand" asp-action="AllsandEdit" asp-route-id="@item.ID" class="btn btn-primary  mx-2"><i class="bi bi-pencil-square"></i>Redigere</a>
                            </div>
                        </td>
                        <td>
                            <div>
                                <a asp-controller="Allsand" asp-action="Delete" asp-route-id="@item.ID" class="btn btn-danger  mx-1"><i class="bi bi-trash3"></i>Slet</a>
                            </div>
                        </td>

                    </tr>
                }

            </tbody>
        </table>
    }

    else
    {
        <p> Item Not Exist </p>
    }

</div>






