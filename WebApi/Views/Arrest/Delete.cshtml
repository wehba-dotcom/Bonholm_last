            <div class="border p-3">
                <div class="col">
                    <div class="row-12">
                        <div class="form-group col">
                            <div class="mp-3">
                                <label asp-for="Efternavn" class="form-label"></label>
                                <input asp-for="Efternavn" disabled class="form-control" />
                           
                            </div>
                            <div class="mp-3">
                                <label asp-for="Fornavne" class="form-label"></label>
                                <input asp-for="Fornavne" disabled class="form-control" />
                                
                            </div>
                            <div class="mp-3">
                                <label asp-for="Fødested" class="form-label"></label>
                                <input asp-for="Fødested" disabled class="form-control" />
                                
                            </div>


                            <div class="mp-3">
                                <label asp-for="Alder" class="form-label"></label>
                                <input asp-for="Alder" disabled class="form-control" />
                               
                            </div>
                            <div class="mp-3">
                                <label asp-for="GiftUgift" class="form-label"></label>
                                <input asp-for="GiftUgift" disabled class="form-control" />
                            
                            </div>

                            <div class="mp-3">
                                <label asp-for="Stilling" class="form-label"></label>
                                <input asp-for="Stilling" disabled class="form-control" />
                             
                            </div>

                            <div class="mp-3">
                                <label asp-for="Amt" class="form-label"></label>
                                <input asp-for="Amt" disabled class="form-control" />
                               
                            </div>

                            <div class="mp-3">
                                <label asp-for="SognAdresse" class="form-label"></label>
                                <input asp-for="SognAdresse" disabled class="form-control" />
                              
                            </div>

                            <div class="mp-3">
                                <label asp-for="Løbenr" class="form-label"></label>
                                <input asp-for="Løbenr" disabled class="form-control" />
                                
                            </div>

                            <div class="mp-3">
                                <label asp-for="IndtastNr" class="form-label"></label>
                                <input asp-for="IndtastNr" disabled class="form-control" />
                                
                            </div>

                            <div class="mp-3">
                                <label asp-for="Bemærkninger" class="form-label"></label>
                                <input asp-for="Bemærkninger" disabled class="form-control" />
                           
                            </div>




                            <br />
                            <div class="form-group row">
                                <div class="col-8 offset-2 row">
                                    <div class="col">
                                        <input type="submit" class="btn btn-primary w-75" value="Slet" />
                                    </div>
                                    <div class="col">
                                        <a asp-action="Index" class="btn btn-primary w-75">TilBage</a>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
        </form>
    </div>
</div>



