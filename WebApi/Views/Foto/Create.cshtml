@model WebApi.SharedModels.Foto

<div class="card shadow border-0 mt-4">
    <div class=" card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">

                <h2 class="text-blabk-50 pl-3">TilFøjet Ny </h2>

            </div>

        </div>

    </div>
    <div class="card-body p-4">
        <form method="post" action="Create">
            <div class=" border p-3">
                <div class="form-group row text-center "></div>
                <div class="col">
                    <div class="row-12">
                        <div class="form-group col">
                            <div asp-validation-summary="All"></div>
                            <div class="mp-3">
                                <label asp-for="Fornavn" class="form-label"></label>
                                <input asp-for="Fornavn" class="form-control" />
                                <span asp-validation-for="Fornavn" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Efternavn" class="form-label"></label>
                                <input asp-for="Efternavn" class="form-control" />
                                <span asp-validation-for="Efternavn" class="text-danger"></span>
                            </div>
                           
                          
                            <div class="mp-3">
                                <label asp-for="Stilling" class="form-label"></label>
                                <input asp-for="Stilling" class="form-control" />
                                <span asp-validation-for="Stilling" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Adr" class="form-label"></label>
                                <input asp-for="Adr" class="form-control" />
                                <span asp-validation-for="Adr" class="text-danger"></span>
                            </div>
                           
                            <div class="mp-3">
                                <label asp-for="Sogn" class="form-label"></label>
                                <input asp-for="Sogn" class="form-control" />
                                <span asp-validation-for="Sogn" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Dato" class="form-label"></label>
                                <input asp-for="Dato" class="form-control" />
                                <span asp-validation-for="Dato" class="text-danger"></span>
                            </div>

                            <div class="mp-3">
                                <label asp-for="Gange" class="form-label"></label>
                                <input asp-for="Gange" class="form-control" />
                                <span asp-validation-for="Gange" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Billedetype" class="form-label"></label>
                                <input asp-for="Billedetype" class="form-control" />
                                <span asp-validation-for="Billedetype" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="BilledeNr" class="form-label"></label>
                                <input asp-for="BilledeNr" class="form-control" />
                                <span asp-validation-for="BilledeNr" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Fotoår" class="form-label"></label>
                                <input asp-for="Fotoår" class="form-control" />
                                <span asp-validation-for="Fotoår" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="Fotograf" class="form-label"></label>
                                <input asp-for="Fotograf" class="form-control" />
                                <span asp-validation-for="Fotograf" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="FotografsNr" class="form-label"></label>
                                <input asp-for="FotografsNr" class="form-control" />
                                <span asp-validation-for="FotografsNr" class="text-danger"></span>
                            </div>
                            <div class="mp-3">
                                <label asp-for="CdNr" class="form-label"></label>
                                <input asp-for="CdNr" class="form-control" />
                                <span asp-validation-for="CdNr" class="text-danger"></span>
                            </div
                             <div class="mp-3">
                                <label asp-for="Noter" class="form-label"></label>
                                <input asp-for="Noter" class="form-control" />
                                <span asp-validation-for="Noter" class="text-danger"></span>
                            </div>
                             <div class="mp-3">
                                <label asp-for="Bemærkning" class="form-label"></label>
                                <input asp-for="Bemærkning" class="form-control" />
                                <span asp-validation-for="Bemærkning" class="text-danger"></span>
                            </div>
                             <div class="mp-3">
                                <label asp-for="Billede" class="form-label"></label>
                                <input asp-for="Billede" class="form-control" />
                                <span asp-validation-for="Billede" class="text-danger"></span>
                            </div>
                            



                        </div>
                        <br />
                        <div class="row">
                            <div class="col-6 col-md-3">

                                <input type="submit" class="btn btn-primary w-75" value="Tilføj" />
                            </div>
                            <div class="col-6 col-md-3">
                                <a asp-action="Index" class="btn btn-primary w-75">Tilbage</a>
                            </div>

                        </div>

                </div>

            </div>

        </form>

    </div>

</div>
